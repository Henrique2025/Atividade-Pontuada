programa {
  funcao inicio() {
     // Desvolvendo variaveis
      
      inteiro a, b, c, resultado
     
     escreva("Digite o Valor de A :")
     leia(a)
     escreva("Digite o Valor de B :")
     leia(b)
     escreva("Digite o Valor de C: ")
     leia(c)
    
    // calculando
    
    resultado= a + b

  se("a+b > c"){
    escreva("\nresultado maior que C")}
  
  senao{
  ("\nresultado menor que C")}
  
//exibindo dados
escreva("valor de a+b :", resultado)


  }
}
